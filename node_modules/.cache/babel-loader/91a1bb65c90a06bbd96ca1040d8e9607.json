 </v-list-item>\r\n      </v-list>\r\n    </v-navigation-drawer>\r\n    <!-- 탑색서랍이 툴바 아래에 위치할 때 메뉴 아이콘이 적절히 좌측에\r\n\t\t\t배치되도록 app과 clipped-left 어트리뷰트 지정 -->\r\n    <v-app-bar app clipped-left fixed color=\"primary\" dark>\r\n      <v-app-bar-nav-icon @click.stop=\"drawer =\r\n\t\t\t\t!drawer\"></v-app-bar-nav-icon>\r\n      <v-toolbar-title>푸시알림 구독</v-toolbar-title>\r\n    </v-app-bar>\r\n    <v-content>\r\n      <div class=\"mt-5\"></div>\r\n      <router-view />\r\n    </v-content>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    data() {\r\n      // 탐색서랍과 툴바 메뉴명 항목 배열 반환\r\n      return {\r\n        drawer: false,\r\n        items: [{\r\n            icon: 'notifications',\r\n            title: '푸시알림 등록 및 해지',\r\n            to: '/'\r\n          },\r\n          {\r\n            icon: 'sms',\r\n            title: '푸시알림 메시지 보내기',\r\n            to: '/pushnotify'\r\n          }\r\n        ]\r\n      }\r\n    },\r\n    name: 'App'\r\n  }\r\n</script>"],"sourceRoot":"src"}]}